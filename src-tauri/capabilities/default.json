{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "log:default",
    "process:default",
    "core:window:allow-start-dragging",
    "core:window:allow-set-fullscreen",
    "updater:default",
    "fs:scope-home",
    "fs:allow-home-read",
    "fs:allow-home-write",
    "fs:scope-home-index",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write-recursive",
    "fs:scope-home-recursive",
    "fs:allow-rename",
    "fs:allow-remove",
    "fs:write-files",
    "fs:read-files",
    "dialog:default"
  ],
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ]
}