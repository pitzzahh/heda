	"productName": "Hierarchical Electrical Design Analyzer",
	"version": "0.1.21",
	"identifier": "me.araopj.heda",
	"build": {
		"frontendDist": "../build",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "bun run dev",
		"beforeBuildCommand": "bun run build"
	},
	"app": {
		"windows": [
			{
				"title": "Hierarchical Electrical Design Analyzer v0.1.21",
				"width": 950,
				"height": 650,
				"minWidth": 950,
				"minHeight": 650,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"createUpdaterArtifacts": true,
		"active": true,
		"targets": "all",
		"windows": {
			"webviewInstallMode": {
				"type": "embedBootstrapper"
			}
		},
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"updater": {
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDNEQTI3MjE1QjQ4NTQ4RDgKUldUWVNJVzBGWEtpUGNLMnFHdGtVOTBhOEZ2U2tzN3psSDBvN0xjVDZhUld1RndhU0FTTStFVVAK",
			"endpoints": [
				"https://github.com/pitzzahh/heda/releases/latest/download/latest.json"
