{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "Voltix",
	"version": "0.0.7",
	"identifier": "me.araopj.heda",
	"build": {
		"frontendDist": "../build",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "bun run dev",
		"beforeBuildCommand": "bun run build"
	},
	"app": {
		"windows": [
			{
				"title": "Voltix",
				"width": 800,
				"height": 600,
				"minWidth": 800,
				"minHeight": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
